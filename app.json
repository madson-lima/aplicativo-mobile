{
  "expo": {
    "name": "meu_bairro_mobile",
    "slug": "meu_bairro_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "meu_bairro_mobile",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.madson_lima.meu_bairro_mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Precisamos acessar sua localização para fornecer informações de geolocalização.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you.",
        "FacebookAppId": "1180770262990332",
        "FacebookDisplayName": "Meu Bairro",
        "LSApplicationQueriesSchemes": [
          "fbapi",
          "fb-messenger-share-api",
          "fbauth2",
          "fbshareextension"
        ]
      }
    },
    "android": {
      "package": "com.madson_lima.meu_bairro_mobile",
      "permissions": ["ACCESS_FINE_LOCATION", "ACCESS_COARSE_LOCATION", "FOREGROUND_SERVICE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
      
    "plugins": [
      [
        "expo-facebook",
        {
          "appId": "1180770262990332"
        }
      ],
      "expo-location",
      "expo-notifications",
      "expo-build-properties"
    ]
  }
}
